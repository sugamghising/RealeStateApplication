# Database Configuration
DB_NAME=realstate_db
DB_USER=realstate_user
DB_PASSWORD=your_secure_password_here
DB_ROOT_PASSWORD=your_root_password_here
DB_HOST=localhost
DB_PORT=3306

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
